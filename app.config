<?xml version="1.0"?>
<configuration>

  <appSettings>

	<add key="RaycastingShaderPath" value="Shaders/shader.frag.c"></add>

	<!-- Model orientation -->
	<add key="RotateX" value="100.0"></add>
	<add key="RotateY" value="150.0"></add>
	<add key="RotateZ" value="180.0"></add>
	<add key="TranslateZ" value="-1.0"></add>

	<add key="RenderCubeSideLength" value="0.75"></add>

	<!-- Volume Data file path -->
	<add key ="VolumeDataPath" value="../../Data/panoramix_256x256x144.raw"/>
	<!--<add key ="VolumeDataPath" value="../../Data/Carp8bit.raw"/>-->

  </appSettings>

  <startup>
	<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>

</configuration>